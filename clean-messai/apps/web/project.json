{
  "name": "web",
  "root": "apps/web",
  "sourceRoot": "apps/web",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/.next"],
      "options": {
        "command": "next build",
        "cwd": "apps/web"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "next dev -p 3001",
        "cwd": "apps/web"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "next lint",
        "cwd": "apps/web"
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "apps/web"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "next start -p 3001",
        "cwd": "apps/web"
      },
      "dependsOn": ["build"]
    }
  },
  "tags": []
}